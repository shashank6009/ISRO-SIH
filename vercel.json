{
  "version": 2,
  "builds": [
    {
      "src": "src/genesis_ai/app/competition_dashboard.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/genesis_ai/inference/service.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/genesis_ai/inference/service.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/genesis_ai/app/competition_dashboard.py"
    }
  ],
  "env": {
    "PYTHONPATH": "src"
  }
}

{
  "name": "genesis-ai",
  "version": "1.0.0",
  "description": "GENESIS-AI GNSS Error Forecasting Platform",
  "main": "index.js",
  "scripts": {
    "dev": "python3 -m streamlit run src/genesis_ai/app/main.py --server.port=8501",
    "competition": "python3 -m streamlit run src/genesis_ai/app/competition_dashboard.py --server.port=8502",
    "start": "python3 -m streamlit run src/genesis_ai/app/main.py --server.port=8501 --server.headless=true",
    "api": "python3 -m uvicorn genesis_ai.inference.service:app --host 0.0.0.0 --port 8000",
    "serve": "concurrently \"npm run api\" \"npm run start\"",
    "serve-competition": "concurrently \"npm run api\" \"npm run competition\"",
    "install-deps": "python3 -m pip install -e .",
    "test": "python3 -m pytest -v",
    "lint": "python3 -m ruff check src tests",
    "format": "python3 -m black src tests && python3 -m isort src tests"
  },
  "keywords": [
    "gnss",
    "satellite",
    "forecasting",
    "ai",
    "machine-learning",
    "isro"
  ],
  "author": "ISRO GNSS Research Division",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isro/genesis-ai.git"
  },
  "bugs": {
    "url": "https://github.com/isro/genesis-ai/issues"
  },
  "homepage": "https://github.com/isro/genesis-ai#readme"
}
